import java.io.File;
import java.io.FileOutputStream;
import java.io.IOException;


public class FileWriter {
	public static void writeIntegers(File f, int[] a){
			FileOutputStream op = new FileOutputStream(f);
			op.write(0xDE);
			op.write(0xAD);
			op.write(0xBE);
			op.write(0xEF);
			op.close();

catch (IOException e) {
			e.printStackTrace();
	}
}
