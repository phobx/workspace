import java.io.File;
import java.io.IOException;

public class FilesTwoMain {

	public static void main(String[] args) {
		File dir = new File("dir");
		dir.mkdirs();
		
		File f = new File(dir, "TimesTable");

		try {
			if (!f.exists()) {
				f.createNewFile();
			}
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
